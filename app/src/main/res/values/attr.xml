<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <declare-styleable name="AutoScrollViewPager">

        <attr name="point_layout" format="string"/>

    </declare-styleable>
    <attr name="strokeColor" format="color" />
    <attr name="speed">
        <enum name="speedOne" value="1" />
        <enum name="speedTwo" value="2" />
    </attr>
    <style name="CustomCheckBoxTheme" parent="@android:style/Widget.CompoundButton.CheckBox">
           <item name="android:button">@drawable/shopcar_checkbox_bg</item>
           </style>
    <declare-styleable name="ProgressLayout">
        <attr name="progressLoadingStateBackgroundColor" format="color" />
        <attr name="progressEmptyStateBackgroundColor" format="color" />
        <attr name="progressErrorStateBackgroundColor" format="color" />
    </declare-styleable>
    <declare-styleable name="qr_ViewfinderView">
        <attr name="qr_angleColor" format="color"/>
        <attr name="qr_hint" format="string"/>
        <attr name="qr_textHintColor" format="color"/>
        <attr name="qr_errorHint" format="string"/>
        <attr name="qr_textErrorHintColor" format="color"/>
        <attr name="qr_offsetX" format="integer"/>
        <attr name="qr_offsetY" format="integer"/>
        <attr name="qr_showPossiblePoint" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="RightDiaView">
        <attr name="strokeColor" />
        <attr name="speed" />
    </declare-styleable>
    <style name="Widget.MyApp.ListView" parent="@android:style/Widget.ListView">
        <item name="android:paddingTop">0dp</item>
        <item name="android:paddingBottom">0dp</item>
    </style>
    <declare-styleable name="WrongDiaView">
        <attr name="strokeColor" />
        <attr name="speed" />
    </declare-styleable>
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <!-- {@deprecated Use civ_circlebackground_color instead.} -->
        <attr name="civ_fill_color" format="color" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>
    <style name="cartdialog">
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowActionModeOverlay">true</item>
    </style>
    <declare-styleable name="RatingBar">
        <!--未选中-->
        <attr name="starEmptyRes" format="reference" />
        <!--选中半个-->
        <attr name="starHalfRes" format="reference" />
        <!--选中全部-->
        <attr name="starSelectedRes" format="reference" />
        <!--星星的总数-->
        <attr name="startTotalNumber" format="integer" />
        <!--默认选中的数量 -->
        <attr name="selectedNumber" format="float" />
        <!--星星的间距-->
        <attr name="starDistance" format="dimension" />
        <!--星星的宽度-->
        <attr name="starWidth" format="dimension" />
        <!--星星的高度-->
        <attr name="starHeight" format="dimension" />
        <!--是不是只画整个星星，不画半个-->
        <attr name="starIsFull" format="boolean" />
    </declare-styleable>
    <dimen name="radio_button_conner_radius">5dp</dimen>
    <dimen name="radio_button_stroke_border">1dp</dimen>

    <color name="radio_button_selected_color">@android:color/tab_indicator_text</color>
    <color name="radio_button_unselected_color">@android:color/transparent</color>


    <declare-styleable name="SegmentedGroup">
        <attr name="sc_corner_radius" format="dimension" />
        <attr name="sc_border_width" format="dimension" />
        <attr name="sc_tint_color" format="color" />
        <attr name="sc_checked_text_color" format="color" />
    </declare-styleable>

    <style name="RadioButton">
        <item name="android:textColor">@color/blue</item>
        <item name="android:minHeight">33dp</item>
        <item name="android:minWidth">70dp</item>
        <item name="android:gravity">center</item>
        <item name="android:button">@null</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
    </style>


    <declare-styleable name="CouponView">
        <attr name="cv_semicircle_gap" format="dimension|reference"/>
        <attr name="cv_semicircle_radius" format="dimension|reference"/>
        <attr name="cv_semicircle_color" format="color|reference"/>
        <attr name="cv_dash_line_length" format="dimension|reference"/>
        <attr name="cv_dash_line_gap" format="dimension|reference"/>
        <attr name="cv_dash_line_height" format="dimension|reference"/>
        <attr name="cv_dash_line_color" format="color|reference"/>
        <attr name="cv_is_semicircle_top" format="boolean|reference"/>
        <attr name="cv_is_semicircle_bottom" format="boolean|reference"/>
        <attr name="cv_is_semicircle_left" format="boolean|reference"/>
        <attr name="cv_is_semicircle_right" format="boolean|reference"/>
        <attr name="cv_is_dash_line_top" format="boolean|reference"/>
        <attr name="cv_is_dash_line_bottom" format="boolean|reference"/>
        <attr name="cv_is_dash_line_left" format="boolean|reference"/>
        <attr name="cv_is_dash_line_right" format="boolean|reference"/>
        <attr name="cv_dash_line_margin_top" format="dimension|reference"/>
        <attr name="cv_dash_line_margin_bottom" format="dimension|reference"/>
        <attr name="cv_dash_line_margin_left" format="dimension|reference"/>
        <attr name="cv_dash_line_margin_right" format="dimension|reference"/>
    </declare-styleable>
    <style name="MyDialog" parent="android:style/Theme.Dialog">
        <!--背景颜色及和透明程度-->
        <item name="android:windowBackground">@color/white</item>
        <!--是否去除标题 -->
        <item name="android:windowNoTitle">true</item>
        <!--是否去除边框-->
        <item name="android:windowFrame">@null</item>
        <!--是否浮现在activity之上-->
        <item name="android:windowIsFloating">true</item>
        <!--是否模糊-->
        <item name="android:backgroundDimEnabled">true</item>
    </style>
</resources>